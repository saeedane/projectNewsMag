{
  "site_layout_style": "boxed",
  "site_width": "1180px",
  "site_boxed_inner_width": "1220px",
  "site_bg_image": {
    "img": "%attachment_url%ak-media-main-bg%attachment_url%",
    "type": "cover"
  },
  "builder_header_desktop": {
    "order": ["top", "mid", "bottom"],
    "top": {
      "scheme": "dark",
      "left": {
        "layout": "normal",
        "align": "left",
        "items": ["top_menu", "divider1", "date"]
      },
      "center": { "layout": "normal", "align": "center" },
      "right": {
        "layout": "grow",
        "align": "right",
        "items": ["search_form", "social_icons", "dark_mode"]
      }
    },
    "mid": {
      "scheme": "dark",
      "left": { "layout": "normal", "align": "left", "items": ["logo"] },
      "center": { "layout": "normal", "align": "left" },
      "right": { "layout": "grow", "align": "right", "items": ["ad1"] }
    },
    "bottom": {
      "scheme": "dark",
      "left": { "layout": "grow", "align": "left", "items": ["main_menu"] },
      "center": { "layout": "normal", "align": "center" },
      "right": { "layout": "normal", "align": "right", "items": ["user"] }
    }
  },
  "builder_header_mobile": {
    "order": ["mobile"],
    "mobile": {
      "left": { "layout": "grow", "align": "left", "items": ["menu_handler"] },
      "center": {
        "layout": "grow",
        "align": "center",
        "items": ["mobile_logo"]
      },
      "right": { "layout": "grow", "align": "right", "items": ["search"] }
    }
  },
  "builder_header_sticky": {
    "order": ["sticky"],
    "sticky": {
      "scheme": "dark",
      "left": { "layout": "grow", "align": "left", "items": ["main_menu"] },
      "center": { "layout": "normal", "align": "left", "items": ["main_menu"] },
      "right": { "layout": "normal", "align": "right", "items": ["search"] }
    }
  },
  "builder_drawer": {
    "order": ["drawer"],
    "drawer": {
      "left": { "layout": "grow", "align": "left", "items": ["mobile_logo"] },
      "center": {
        "layout": "normal",
        "align": "center",
        "items": ["search_form", "mobile_menu"]
      },
      "right": { "layout": "grow", "align": "right", "items": ["social_icons"] }
    }
  },
