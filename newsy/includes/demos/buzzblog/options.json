  "builder_header_desktop": {
    "order": ["top", "mid", "bottom"],
    "mid": {
      "layout": "stretched",
      "left": {
        "items": ["menu_handler", "social_icons"]
      },
      "center": {
        "items": ["logo"]
      },
      "right": {
        "items": ["dark_mode", "search", "button1", "shop", "user"]
      }
    }
  },
  "builder_header_mobile": {
    "mobile": {
      "left": {
        "items": ["menu_handler"]
      },
      "center": {
        "items": ["mobile_logo"]
      },
      "right": {
        "items": ["search"]
      }
    }
  },
  "builder_header_sticky": {
    "sticky": {
      "layout": "stretched",
      "left": {
        "items": ["menu_handler"]
      },
      "center": {
        "items": ["mobile_logo"]
      },
      "right": {
        "items": ["search"]
      }
    }
  },
  "builder_drawer": {
    "drawer": {
      "left": {
        "items": ["button1", "badge_menu"]
      },
      "center": {
        "layout": "grow",
